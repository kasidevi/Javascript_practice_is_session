###ARRAYS JS###

let myArray = [+3, -4.5, 7, "js", -98, 'A'];
myArray[10] = 10; //add ele 10 to the index 10    
alert(myArray); //print arr up to index 9
alert(myArray.length); //len is 10
                                                                                            
